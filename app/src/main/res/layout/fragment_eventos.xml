<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:aplicativo="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".FragmentEventos"
    aplicativo:cardCornerRadius="20dp">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <ListView
            android:id="@+id/lista_eventos"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:text="Fragment Eventos funcionando"
            android:divider="@android:color/transparent"
            android:dividerHeight="4.0sp"
            android:padding="5dp"
            android:scrollbars="vertical"
            tools:listitem="@layout/list_item" />

    </androidx.constraintlayout.widget.ConstraintLayout>

</FrameLayout>
